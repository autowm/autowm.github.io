.jasa-container {display:inline-block}
 a.jasa {float:left;width:50%;color:black;height:200px}
  .jasa.loading .jasa-pembungkus {background:#ffffff}
  .jasa.seo .jasa-pembungkus {background:#ffffff}
  .jasa.tanya .jasa-pembungkus {background:#ffffff}
  .jasa.template .jasa-pembungkus {background:#ffffff}
